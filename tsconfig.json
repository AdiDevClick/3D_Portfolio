{
  "files": [],
  "compilerOptions": {
      "composite": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": false,
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": ".",
      "allowJs": true,
      "checkJs": true,
      "paths": {
        "@/*": ["src/*"],
        "@assets/*": ["src/assets/*"],
        "@css/*": ["src/assets/css/*"],
        "@images/*": ["src/assets/images/*"],
        "@models/*": ["src/3DModels/*"],
        "@data/*": ["src/data/*"],
        "@utils/*": ["src/utils/*"],
        "@hooks/*": ["src/hooks/*"]
      },
      "types": ["react", "react-dom"],
      "lib": ["DOM", "ESNext"],
  },
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "include": ["src/**/*"],
  "exclude": ["node_modules"] 
}
